<!DOCTYPE html>
<html>
<head>
    <meta charset='utf-8'>
    <link rel="stylesheet" href="styles.css">
    <title>Conections</title>
  
</head>
<body>  
    
 
    <div id='title'>
        <h1> Datos de usuarios</h1>
    </div>
 
   
    <div>
        <table>
            <th>#</th>
            <th>Tag</th>
            <th>id</th>
            <th>user_id</th>
            <th>Productos comprados</th>
            <th>Fecha compra</th>
            <th>Total pagado</th>

            <% for(var i=0; i<orders.length; i++){ %>
                <tr>
                    <td><%=i %> </td>
                    <td><%= orders[i].tag %> </td>
                    <td><%= orders[i].id %> </td>
                    <td><%= orders[i].id_customer %> </td>
                    <td><%= orders[i].products %> </td>
                    <td> <%= orders[i].invoice_date %> </td>
                    <td><%= orders[i].total_paid %> </td>     
                </tr>
                <% } %>
          
                
        </table>

    </div>



    
</body>
</html>
