<!DOCTYPE html>
<html>
<head>
    <meta charset='utf-8'>
    <link rel="stylesheet" href="styles.css">
    <title>Conections</title>
  
</head>
<body>  
    
 
    <div  class="topnav">
    
        <h3 id="title"> Orders</h3>
    
        <ul id=”button”>
            <li><a href="/">Home</a></li>
            <li><a href="/connections">Connections</a></li>
            <li><a href="/amoconnections">AmoCrm connections</a></li>
            <li><a href="/users">Users</a></li>
            <li><a href="/addresses">Addresses</a></li>
            <li><a href="/orders">Orders</a></li>
            <li><a href="/amoContacts">amoContacts</a></li>
        
             
        </ul>
        
  </div>
   
    <div>
        <table>
            <th>#</th>
            <th>Tag</th>
            <th>id</th>
            <th>user_id</th>
            <th>Productos comprados</th>
            <th>Fecha compra</th>
            <th>Total pagado</th>

            <% for(var i=0; i<orders.length; i++){ %>
                <tr>
                    <td><%=i %> </td>
                    <td><%= orders[i].tag %> </td>
                    <td><%= orders[i].id %> </td>
                    <td><%= orders[i].id_customer %> </td>
                    <td><%= orders[i].products %> </td>
                    <td> <%= orders[i].invoice_date %> </td>
                    <td><%= orders[i].total_paid %> </td>     
                </tr>
                <% } %>
          
                
        </table>

    </div>



    
</body>
</html>
