<!DOCTYPE html>
<html>
<head>
    <meta charset='utf-8'>
    <link rel="stylesheet" href="styles.css">
    <title>Conections</title>
  
</head>
<body>  
    
 
    <div  class="topnav">
    
        <h3> Addresses</h3>
    
        <ul id=”button”>
            <li><a href="/">Home</a></li>
            <li><a href="/connections">Connections</a></li>
            <li><a href="/users">Users</a></li>
            <li><a href="/addresses">Addresses</a></li>
            <li><a href="/orders">Orders</a></li>
        
             
        </ul>
        
  </div>
 
   
    <div>
        <table>
            <th>#</th>
            <th>Tag</th>
            <th>id</th>
            <th>user_id</th>
            <th>telefono</th>
            <th>celular</th>
            <th>dirección</th>
            <th>CP</th>
            <th>Ciudad</th>

            <% for(var i=0; i<addresses.length; i++){ %>
                <tr>
                    <td><%=i %> </td>
                    <td><%= addresses[i].tag %> </td>
                    <td><%= addresses[i].id %> </td>
                    <td><%= addresses[i].id_customer %> </td>
                    <td> <%= addresses[i].phone %> </td>
                    <td><%= addresses[i].cellPhone %> </td>
                    <td> <%= addresses[i].address %> </td>
                    <td><%= addresses[i].postCode %> </td>
                    <td> <%= addresses[i].city %> </td>

                </tr>
                <% } %>
          
                
        </table>

    </div>



    
</body>
</html>
